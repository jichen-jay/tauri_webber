{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "tauri-chat",
  "version": "0.1.0",
  "identifier": "com.tauri.dev0",
  "build": {
    "frontendDist": [
      "../dist"
    ],
    "devUrl": "http://localhost:8080"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tauri-chat",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}